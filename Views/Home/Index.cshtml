@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="slider_section">
    <div id="myCarousel" class="carousel slide banner-main" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="first-slide" src="images/Ip11.png" alt="First slide" width="1920" height="836">

            </div>
            <div class="carousel-item">
                <img class="second-slide" src="images/ss.png" alt="Second slide" width="1920" height="836">

            </div>
            <div class="carousel-item">
                <img class="third-slide" src="images/A73.png" alt="Third slide" width="1920" height="836">
            </div>
        </div>
        <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
            <i class='fa fa-angle-left'></i>
        </a>
        <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
            <i class='fa fa-angle-right'></i>
        </a>
    </div>
    @Html.Raw(TempData["msgdh"])
</section>
